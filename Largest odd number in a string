class Solution {
    public String largestOddNumber(String num) {
             int n = num.length();
        int end = -1;
        for (int i = n - 1; i >= 0; i--) {
            int digit = num.charAt(i) - '0';
            if (digit % 2 != 0) {
                end = i;
                break;
            }
        }
        if (end == -1) {
            return "";
        }

        return num.substring(0, end + 1);
    }
}
